{
  "allow": [
    "^ls( -[a-zA-Z]+)?( .*)?$",
    "^pwd$",
    "^whoami$",
    "^cat [^|;&]+$",
    "^head -n [0-9]+ [^|;&]+$",
    "^tail -n [0-9]+ [^|;&]+$",
    "^sed -n '?[0-9,]*p'? [^|;&]+$",
    "^rg( .*)?$",
    "^find [^|;&]+$",
    "^git status$",
    "^git diff( .*)?$",
    "^git add( .*)?$",
    "^git commit( .*)?$",
    "^git log( .*)?$",
    "^git show( .*)?$",
    "^pytest( .*)?$",
    "^python -m pytest( .*)?$",
    "^coverage run( .*)?$",
    "^coverage report( .*)?$",
    "^ruff( .*)?$",
    "^black( .*)?$",
    "^mypy( .*)?$",
    "^pip install -r requirements.txt$",
    "^pip install -r requirements-dev.txt$",
    "^pip install --no-deps -r requirements.txt$",
    "^poetry install( .*)?$",
    "^poetry run pytest( .*)?$",
    "^uvicorn src.api.main:app --reload --host 0.0.0.0 --port 8080$",
    "^npm run [\\w:-]+$",
    "^pnpm run [\\w:-]+$",
    "^make( .*)?$"
  ]
}
